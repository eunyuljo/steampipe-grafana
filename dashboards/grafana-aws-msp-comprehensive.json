          "rawSql": "-- ë³´ì•ˆ ìœ„í—˜ ìš”ì†Œ ì¢…í•©\nSELECT \n  'EC2 Instance' as resource_type,\n  instance_id as resource_id,\n  'SSH Open to 0.0.0.0/0' as issue,\n  'HIGH' as risk_level,\n  region,\n  placement_availability_zone as az\nFROM aws_ec2_instance \nWHERE instance_id IN (\n  SELECT DISTINCT instance_id \n  FROM aws_ec2_instance_security_group isg\n  JOIN aws_vpc_security_group_rule sgr ON isg.group_id = sgr.group_id\n  WHERE sgr.type = 'ingress' AND sgr.from_port = 22 AND sgr.to_port = 22 AND sgr.cidr_ipv4 = '0.0.0.0/0'\n)\nUNION ALL\nSELECT \n  'RDS Instance' as resource_type,\n  db_instance_identifier as resource_id,\n  'Publicly Accessible' as issue,\n  'HIGH' as risk_level,\n  region,\n  availability_zone as az\nFROM aws_rds_db_instance \nWHERE publicly_accessible = true\nUNION ALL\nSELECT \n  'EBS Volume' as resource_type,\n  volume_id as resource_id,\n  'Not Encrypted' as issue,\n  'MEDIUM' as risk_level,\n  region,\n  availability_zone as az\nFROM aws_ebs_volume \nWHERE encrypted = false\nORDER BY \n  CASE risk_level \n    WHEN 'HIGH' THEN 1 \n    WHEN 'MEDIUM' THEN 2 \n    WHEN 'LOW' THEN 3 \n  END,\n  resource_type",
