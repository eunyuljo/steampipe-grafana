      "pluginVersion": "12.3.3",
      "targets": [
        {
          "datasource": {
            "type": "postgres",
            "uid": "steampipe"
          },
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "-- Í≥ÑÏ†ïÎ≥Ñ Î≥¥Ïïà ÌòÑÌô© Ï¢ÖÌï©\nWITH security_summary AS (\n  SELECT \n    connection_name,\n    COUNT(*) FILTER (WHERE encrypted = false) as unencrypted_volumes\n  FROM aws_all.aws_ebs_volume\n  GROUP BY connection_name\n),\nrds_summary AS (\n  SELECT \n    connection_name,\n    COUNT(*) FILTER (WHERE publicly_accessible = true) as public_rds,\n    COUNT(*) FILTER (WHERE backup_retention_period = 0) as no_backup_rds\n  FROM aws_all.aws_rds_db_instance\n  GROUP BY connection_name\n),\nec2_summary AS (\n  SELECT \n    connection_name,\n    COUNT(*) as total_instances,\n    COUNT(*) FILTER (WHERE instance_state = 'stopped') as stopped_instances\n  FROM aws_all.aws_ec2_instance\n  GROUP BY connection_name\n)\nSELECT \n  COALESCE(s.connection_name, r.connection_name, e.connection_name) as connection_name,\n  COALESCE(e.total_instances, 0) as total_instances,\n  COALESCE(e.stopped_instances, 0) as stopped_instances,\n  COALESCE(s.unencrypted_volumes, 0) as unencrypted_volumes,\n  COALESCE(r.public_rds, 0) as public_rds,\n  COALESCE(r.no_backup_rds, 0) as no_backup_rds,\n  CASE \n    WHEN COALESCE(s.unencrypted_volumes, 0) + COALESCE(r.public_rds, 0) = 0 THEN '‚úÖ ÏïàÏ†Ñ'\n    WHEN COALESCE(s.unencrypted_volumes, 0) + COALESCE(r.public_rds, 0) <= 5 THEN '‚ö†Ô∏è Ï£ºÏùò'\n    ELSE 'üö® ÏúÑÌóò'\n  END as security_status\nFROM security_summary s\nFULL OUTER JOIN rds_summary r ON s.connection_name = r.connection_name\nFULL OUTER JOIN ec2_summary e ON COALESCE(s.connection_name, r.connection_name) = e.connection_name\nORDER BY (COALESCE(s.unencrypted_volumes, 0) + COALESCE(r.public_rds, 0)) DESC",
          "refId": "A"
        }
      ],
      "title": "üõ°Ô∏è Í≥ÑÏ†ïÎ≥Ñ Î≥¥Ïïà ÌòÑÌô© ÏöîÏïΩ",
      "type": "table"
